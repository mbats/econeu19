            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="mindstorms::Rotate" referenceName="instructions">
                <subModelOperations xsi:type="tool_1:SetValue" featureName="degrees" valueExpression="-90"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
