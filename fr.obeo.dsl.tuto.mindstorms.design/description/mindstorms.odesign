            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="CD_RotateRight" preconditionExpression="aql:self.degrees&lt;0" semanticCandidatesExpression="feature:instructions" domainClass="mindstorms::Rotate">
