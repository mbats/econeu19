            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="CD_RotateLeft" preconditionExpression="aql:self.degrees>=0" semanticCandidatesExpression="feature:instructions" domainClass="mindstorms::Rotate">
