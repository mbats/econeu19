        <containerMappings name="CD_SubChoreography" semanticCandidatesExpression="feature:instructions" doubleClickDescription="//@ownedViewpoints[name='Mindstorms']/@ownedRepresentations[name='Choreography%20Diagram']/@defaultLayer/@toolSections.0/@ownedTools[name='CD_DoubleClick']" domainClass="mindstorms::Choreography" childrenPresentation="List">
